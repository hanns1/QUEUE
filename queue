#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q;

    q.push(10);
    q.push(20);
    q.push(30);

    cout << "elemen di depan : " << q.front() << endl;  // Output: 10
    cout << "ukuran antrian " << q.size() << endl;      // Output: 3

    q.pop();
    cout << "elemen didepan setelah di pop : " << q.front() << endl;  // Output: 20

    return 0;
}
